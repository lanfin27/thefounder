{
  "name": "stealth-collection-integration-system",
  "version": "1.0.0",
  "description": "Complete integration system with dashboard, database, logging, analytics, and production deployment",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:dev": "NODE_ENV=development node index.js",
    "start:staging": "NODE_ENV=staging node index.js",
    "start:production": "NODE_ENV=production node index.js",
    "test": "jest",
    "lint": "eslint .",
    "validate": "node -e \"require('./production-deployment/deployment-config').validate()\"",
    "generate-env": "node -e \"console.log(new (require('./production-deployment/deployment-config'))().generateEnvTemplate())\""
  },
  "keywords": [
    "integration",
    "dashboard",
    "monitoring",
    "analytics",
    "deployment"
  ],
  "author": "Stealth Collection Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-elasticsearch": "^0.17.4",
    "axios": "^1.6.2",
    "nodemailer": "^6.9.7",
    "chart.js": "^4.4.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}